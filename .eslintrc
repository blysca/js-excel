{
  "parser" : "babel-eslint",
  "env" : {
    "es6" : true,
    "browser" : true,
    "node" : true
  },
  "rules" : {
    "semi": "off",
    "arrow-parens" : "off",
    "object-curly-spacing" : "off",
    "array-bracket-spacing" : "off",
    "comma-dangle" : "off",
    "require-jsdoc" : "off",
    "computed-property-spacing": "off"
  },
  "extends" : [
    "google"
  ]
}
